{
  "plugins": [
    "edit-link",
    "-fontsettings",
    "ga",
    "theme-opspec@git+https://github.com/opspec-io/gitbook-plugin-theme-opspec.git",
    "swagger",
    "toggle-chapters",
    "github-buttons"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/opspec-io/spec/edit/master",
      "label": "edit this page"
    },
    "ga": {
      "token": "UA-79889286-1"
    },
    "github-buttons": {
      "repo": "opspec-io/spec",
      "types": [
        "star",
        "watch"
      ],
      "size": "small"
    }
  },
  "title": "opspec"
}

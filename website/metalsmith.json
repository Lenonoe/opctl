{
  "source": "src",
  "destination": "../build",
  "plugins": {
    "metalsmith-markdown": {
      "langPrefix": "language-",
      "smartypants": true,
      "smartLists": true,
      "gfm": true,
      "tables": true
    },
    "metalsmith-prism": {
      "lineNumbers": true
    },
    "metalsmith-permalinks": {},
    "metalsmith-headings":{},
    "metalsmith-layouts": {
      "engine": "handlebars",
      "partials": "partials"
    },
    "metalsmith-in-place": {
      "engine": "handlebars",
      "pattern": "**/*.html"
    },
    "metalsmith-sass": {
      "outputStyle": "expanded"
    },
    "metalsmith-debug": {}
  }
}

{
  "plugins": [
    "copy-code-button",
    "anchors",
    "include-codeblock",
    "ace",
    "edit-link",
    "-fontsettings",
    "ga",
    "github-buttons",
    "-sharing",
    "toggle-chapters",
    "wide-page"
  ],
  "pluginsConfig": {
    "include-codeblock": {
      "template": "ace"
    },
    "edit-link": {
      "base": "https://github.com/opspec-io/spec/edit/master/docs",
      "label": "edit this page"
    },
    "ga": {
      "token": "UA-79889286-1"
    },
    "github-buttons": {
      "buttons": [
        {
          "type": "star",
          "user": "opspec-io",
          "repo": "spec",
          "size": "small",
          "count": true
        },
        {
          "type": "watch",
          "user": "opspec-io",
          "repo": "spec",
          "size": "small",
          "count": true
        }
      ]
    }
  },
  "structure": {
    "readme": "about.md"
  },
  "title": "opspec"
}

{
  "plugins": [
    "copy-code-button",
    "anchors",
    "include-codeblock",
    "edit-link",
    "-fontsettings",
    "ga",
    "github-buttons",
    "-sharing",
    "toggle-chapters",
    "wide-page"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/opspec-io/spec/edit/master/docs",
